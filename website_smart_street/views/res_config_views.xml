<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_website_config_settings_smart_streets" model="ir.ui.view">
		<field name="name">website.config.settings.form.inherit</field>
		<field name="model">website.config.settings</field>
		<field name="inherit_id" ref="website.view_website_config_settings" />
		<field name="arch" type="xml">
			<group name="social" position="before">
				<group name="smart_street" string="Smart Streets">
					<label for="auth_id" />
					<div name="auth_id">
						<div class="oe_inline">
							<field name="auth_id" placeholder="a7ded9-b357-5d7-55b8-b0b23b4" />
						</div>
					</div>
					<label for="auth_token" />
					<div name="auth_token">
						<div class="oe_inline">
							<field name="auth_token" placeholder="Hs7AXI3dijdsqik38qiIb" />
						</div>
					</div>
				</group>
			</group>
		</field>
	</record>
</odoo>
